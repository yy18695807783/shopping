<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.lenovo.myshapping">

    <!-- 权限 -->
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".app.MyAppliction"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".app.MainActivity"
            android:theme="@style/Theme.PageIndicatorDefaults">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".app.GoodsInfoActivity" />
        <activity android:name=".home.actyvity.GoodsListActivity" />
        <activity android:name=".shoppingcar.activity.ShoppingCartActivity" />
        <activity android:name=".user.activity.MessageCenterActivity" />
        <activity android:name=".app.LoginActivity" />
        <activity android:name=".app.CallCenterActivity" />
    </application>

</manifest>
